# Emmc mount points
/boot                  emmc      /dev/block/platform/bootdevice/by-name/boot                 flags=display="Boot";backup=1;flashimg=1
/dtbo                  emmc      /dev/block/platform/bootdevice/by-name/dtbo                 flags=display="Dtbo";backup=1;flashimg=1
/erecovery_kernel      emmc      /dev/block/platform/bootdevice/by-name/erecovery_kernel     flags=display="eRecovery_kernel";backup=1;flashimg=1
/erecovery_ramdisk     emmc      /dev/block/platform/bootdevice/by-name/erecovery_ramdisk    flags=display="eRecovery_ramdisk";backup=1;flashimg=1
/erecovery_vbmeta      emmc      /dev/block/platform/bootdevice/by-name/erecovery_vbmeta     flags=display="eRecovery_vbmeta";backup=1;flashimg=1
/erecovery_vendor      emmc      /dev/block/platform/bootdevice/by-name/erecovery_vendor     flags=display="eRecovery_vendor";backup=1;flashimg=1
/frp                   emmc      /dev/block/platform/bootdevice/by-name/frp                  flags=display="Frp";backup=1;flashimg=1;wipeingui
/lk                    emmc      /dev/block/platform/bootdevice/by-name/lk                   flags=display="Lk";backup=1;flashimg=1
/misc                  emmc      /dev/block/platform/bootdevice/by-name/misc                 flags=display="Misc";backup=1;flashimg=1;wipeingui
/nvram                 emmc      /dev/block/platform/bootdevice/by-name/nvram                flags=display="Nvram";backup=1;flashimg=1
/oeminfo               emmc      /dev/block/platform/bootdevice/by-name/oeminfo              flags=display="Oeminfo";backup=1;flashimg=1
/preload               emmc      /dev/block/platform/bootdevice/by-name/preload              flags=display="Preload";backup=1;flashimg=1
/proinfo               emmc      /dev/block/platform/bootdevice/by-name/proinfo              flags=display="Proinfo";backup=1;flashimg=1
/recovery_ramdisk      emmc      /dev/block/platform/bootdevice/by-name/recovery_ramdisk     flags=display="Recovery_ramdisk";backup=1;flashimg=1
/recovery_vbmeta       emmc      /dev/block/platform/bootdevice/by-name/recovery_vbmeta      flags=display="Recovery_vbmeta";backup=1;flashimg=1
/recovery_vendor       emmc      /dev/block/platform/bootdevice/by-name/recovery_vendor      flags=display="Recovery_vendor";backup=1;flashimg=1
/vbmeta                emmc      /dev/block/platform/bootdevice/by-name/vbmeta               flags=display="Vbmeta";backup=1;flashimg=1

# Raw mount points
/cache                 ext4      /dev/block/platform/bootdevice/by-name/cache                flags=display="Cache";backup=0;flashimg=0;wipeingui
/cust                  ext4      /dev/block/platform/bootdevice/by-name/cust                 flags=display="Cust";backup=0;flashimg=0;wipeingui
/nvcfg                 ext4      /dev/block/platform/bootdevice/by-name/nvcfg                flags=display="Nvcfg";backup=1;flashimg=0
/nvdata                ext4      /dev/block/platform/bootdevice/by-name/nvdata               flags=display="Nvdata";backup=1;flashimg=0
/odm                   ext4      /dev/block/platform/bootdevice/by-name/odm                  flags=display="Odm";backup=1;flashimg=0;wipeingui
/persist               ext4      /dev/block/platform/bootdevice/by-name/persist              flags=display="Persist";backup=1;flashimg=0;wipeingui
/product               ext4      /dev/block/platform/bootdevice/by-name/product              flags=display="Product";backup=1;flashimg=0;wipeingui
/system                ext4      /dev/block/platform/bootdevice/by-name/system               flags=display="System";backup=1;flashimg=0;wipeingui
/vendor                ext4      /dev/block/platform/bootdevice/by-name/vendor               flags=display="Vendor";backup=1;flashimg=0;wipeingui

# Userdata mount point
/data                  f2fs      /dev/block/platform/bootdevice/by-name/userdata             flags=display="Data";backup=0;flashimg=1;wipeingui

# Removable storage mount points
/external_sd	       auto	     /dev/block/mmcblk1p1	/dev/block/mmcblk1                   flags=display="MicroSD Card";storage;removable;backup=0
/usbotg		           auto	     /dev/block/sda1        /dev/block/sda                       flags=display="USB OTG";storage;removable;backup=0

# Direct image flashing
/product_image               emmc      /dev/block/platform/bootdevice/by-name/product              flags=display="Product image";backup=0;flashimg=1
/system_image                emmc      /dev/block/platform/bootdevice/by-name/system               flags=display="System image";backup=0;flashimg=1
/vendor_image                emmc      /dev/block/platform/bootdevice/by-name/vendor               flags=display="Vendor image";backup=0;flashimg=1
/odm_image                   emmc      /dev/block/platform/bootdevice/by-name/odm                  flags=display="Odm image";backup=0;flashimg=1
